    <children>
      <VBox alignment="CENTER">
         <children>
            <HBox fx:id="topPane" alignment="CENTER" minHeight="100.0" VBox.vgrow="ALWAYS" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                    <TableView fx:id="actionTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
