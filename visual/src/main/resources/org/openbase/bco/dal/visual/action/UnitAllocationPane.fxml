                        <TableColumn fx:id="timestampColumn" prefWidth="193.0" text="Timestamp" />
                            <TableColumn fx:id="actionStateColumn" prefWidth="137.0" text="Action State" />
                            <TableColumn fx:id="serviceStateColumn" prefWidth="238.0" text="Service State" />
